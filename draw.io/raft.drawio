        <mxCell id="F7jCl3piRas6oP6cL2TG-54" value="接收者实现" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="F7jCl3piRas6oP6cL2TG-41" vertex="1">
